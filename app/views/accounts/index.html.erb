<style>
	.wrapper {
		max-width: 920px;
		height: auto;
    margin-left:200px;
	}

	.box {
    padding:20px;
		display: inline-block;
		position: relative;
		width: 200px;
		height: 200px;
		background: #FFF;
		border:1px solid black;
		border-radius:5px;
		z-index: 0;
		margin: 2px 2px 2px -2px;
		transition: all .15s ease-in-out;
	}

	.box:hover {
		background: #9CC;
		z-index: 100;
		transform: scale(1.2,1.2);
		box-shadow: 0 5px 10px 0 rgba(0,0,0,.2);
	}
    @font-face {
    font-family: 'rupee';
