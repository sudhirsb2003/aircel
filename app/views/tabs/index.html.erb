<p style ="float:right;margin-right:50px;"><%= link_to 'New Tab', new_tab_path, :class => "label label-primary" %></p><br>
<table class="table table-striped table-bordered" id="tabs" border="0" cellpadding="0" cellspacing="0" width="100%">
  <thead>
    <tr>
      <th>Name</th>
      <th>Tab code</th>
      <th>Pincode</th>
      <th>Assigned to</th>
      <th>number Of Task</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @tabs.each do |tab| %>
      <tr>
        <td><%= link_to tab.name, tab %></td>
        <td><%= tab.tab_code %></td>
        <td><%= tab.pincode %></td>
        <td><%= tab.user.name if tab.user %></td>
        <td><%= link_to 'Edit', edit_tab_path(tab) %></td>
        <td><%= link_to 'Destroy', tab, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
